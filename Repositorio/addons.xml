<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.program.chikirywizard" name="chikiry wizard" version="0.0.6" provider-name="chikiry">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>executable</provides>
  </extension>
  <extension point="xbmc.addon.repository" name="chikiryrepo">
        <info>https://raw.githubusercontent.com/chikiry/chikiryrepo/master/Repositorio/addons.xml</info>
        <checksum>https://raw.githubusercontent.com/chikiry/chikiryrepo/master/Repositorio/addons.xml.md5</checksum>
        <datadir zip="true">https://raw.githubusercontent.com/chikiry/chikiryrepo/master/Repositorio/</datadir>
    </extension>
  <extension point="xbmc.service" library="startup.py" start="startup" />
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Wizard</summary>
    <description lang="en">chikiry wizard canal de telegram para Novedades y consultas @consultaschikiry.</description>
    <platform>all</platform>
  </extension>
</addon>

<addon id="plugin.video.elpaso"
       name="[COLOR gold]elpaso[/COLOR]"
       version="0.4"
       provider-name="chikiry">
 <requires>
    <import addon="xbmc.python" version="2.1.0"/>
