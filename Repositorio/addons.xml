    <extension point="xbmc.addon.metadata">
    <summary lang="en">Wizard</summary>
    <description lang="en">chikiry wizard canal de telegram para Novedades y consultas @consultaschikiry.</description>
        <platform>all</platform>
        <reuselanguageinvoker>false</reuselanguageinvoker>


