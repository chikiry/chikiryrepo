        <checksum>https://raw.githubusercontent.com/chikiry/chikiryrepo/master/Repositorio/addons.xml.md5</checksum>
        <datadir zip="true">https://raw.githubusercontent.com/chikiry/chikiryrepo/master/Repositorio/</datadir>
   </extension>

   <extension point="xbmc.addon.metadata">
    <summary>Creado por Chikiry, para todos los Cofrades</summary>
    <description>DEDICADO A LA SEMANA SANTA</description>
    <platform>all</platform>
	  <assets>
		<icon>icon.png</icon>
		<fanart>fanart.jpg</fanart>
	  </assets>
  </extension>
</addon>

