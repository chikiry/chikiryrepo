<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.program.chikirywizard" name="chikiry wizard" version="0.2" provider-name="chikiry">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>executable</provides>
  </extension>
  <extension point="xbmc.addon.repository" name="chikiryrepo">
        <info>https://raw.githubusercontent.com/chikiry/chikiryrepo/master/Repositorio/addons.xml</info>
        <checksum>https://raw.githubusercontent.com/chikiry/chikiryrepo/master/Repositorio/addons.xml.md5</checksum>
        <datadir zip="true">https://raw.githubusercontent.com/chikiry/chikiryrepo/master/Repositorio/</datadir>
    </extension>
  <extension point="xbmc.service" library="startup.py" start="startup" />
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Wizard</summary>
    <description lang="en">chikiry wizard canal de telegram para Novedades y consultas @consultaschikiry.</description>
    <platform>all</platform>
  </extension>
</addon>

<addon id="plugin.video.elpaso"
       name="[COLOR gold]elpaso[/COLOR]"
       version="0.1"
       provider-name="chikiry">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
    <import addon="script.module.beautifulsoup" version="3.2.1"/>
    <import addon="script.module.simple.downloader" version="0.9.4"/>
    <import addon="script.module.beautifulsoup4" />
    <import addon="script.module.requests" />
    <import addon="script.module.httplib2" />
    <import addon="script.module.liveresolver" version="0.1.24" optional="true"/>
    <import addon="script.module.youtube.dl" />
    <import addon="plugin.video.youtube"  />
    <import addon="script.module.urlresolver"/>
    <import addon="script.module.simplejson" />
     <import addon="script.module.livestreamer" />
     <import addon="script.module.pyamf" optional="true"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>video</provides>
  </extension>
    <extension point="xbmc.addon.repository" name="chikiryrepo">
        <info>https://raw.githubusercontent.com/chikiry/chikiryrepo/master/Repositorio/addons.xml</info>
        <checksum>https://raw.githubusercontent.com/chikiry/chikiryrepo/master/Repositorio/addons.xml.md5</checksum>
        <datadir zip="true">https://raw.githubusercontent.com/chikiry/chikiryrepo/master/Repositorio/</datadir>
    </extension>
  <extension point="xbmc.addon.metadata">
    <summary>El Paso</summary>
	<description>Primer addon cofrade para kodi, directos, videos, marchas, cofradias,etc....todo desde tu media center</description>
    <platform>all</platform>
  </extension>
</addon>

